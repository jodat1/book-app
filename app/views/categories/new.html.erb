<h1 align ="center">Create new category</h1>

<div align="center">
<div class="mb-3 col-md-8">
    <%= form_with model: @category do |f|%>
    <%= f.label "Category Name", class:"form-label"%>
    <%= f.text_field :name, placeholder: "Enter category name", class:"form-control"%>
</div>
    <%= f.submit class:"btn btn-primary"%>
    <%end%>
</div>